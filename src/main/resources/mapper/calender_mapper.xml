<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC   "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"
>

<mapper namespace="kr.co.porkandspoon.dao.CalenderDAO">

	<!-- 캘린더 데이터 조회 -->
	<select id="calenderList">
		SELECT * FROM schedule
	</select>
	
	<insert id="calenderWrite" parameterType="map">
		INSERT INTO schedule(username,start_date,end_date,subject,content,type)
							VALUES('테스트네임1',#{start_date},#{end_date},#{subject},#{content},'P')
	</insert>
	
</mapper>